{
  "name": "smsts",
  "version": "1.0.0",
  "description": "typescript implementation of sms management api",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "copy-public": "cp -r src/public dist",
    "start": "npm run build && npm run copy-public && nodemon ./dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "mocha --exit --require ts-node/register src/**/*.spec.ts",
    "test-coverage": "nyc mocha --exit --require ts-node/register src/**/*.spec.ts",
    "test-dev": "nodemon --ext ts --exec 'mocha --require ts-node/register src/**/*.spec.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elbertbiggs360/smsts.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Bruce Bigirwenkya (brucebigirwenkya@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Elbertbiggs360/smsts/issues"
  },
  "homepage": "https://github.com/Elbertbiggs360/smsts#readme",
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/mongoose": "^5.5.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "mongoose": "^5.5.11",
    "nodemon": "^1.19.1",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "ts-node": "^8.3.0"
  }
}
